<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">


    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>servlets.LoginServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/LoginServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>FlightServlet</servlet-name>
        <servlet-class>servlets.FlightServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>FlightServlet</servlet-name>
        <url-pattern>/FlightServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ReserveFlightServlet</servlet-name>
        <servlet-class>servlets.ReserveFlightServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ReserveFlightServlet</servlet-name>
        <url-pattern>/ReserveFlightServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>HotelServlet</servlet-name>
        <servlet-class>servlets.HotelServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>HotelServlet</servlet-name>
        <url-pattern>/HotelServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ReserveHotelServlet</servlet-name>
        <servlet-class>servlets.ReserveHotelServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ReserveHotelServlet</servlet-name>
        <url-pattern>/ReserveHotelServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ConferenceServlet</servlet-name>
        <servlet-class>servlets.ConferenceServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ConferenceServlet</servlet-name>
        <url-pattern>/ConferenceServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ReserveConferenceServlet</servlet-name>
        <servlet-class>servlets.ReserveConferenceServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ReserveConferenceServlet</servlet-name>
        <url-pattern>/ReserveConferenceServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>home</servlet-name>
        <jsp-file>/home.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>home</servlet-name>
        <url-pattern>/home</url-pattern>
    </servlet-mapping>



</web-app>